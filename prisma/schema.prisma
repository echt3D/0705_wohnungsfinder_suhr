generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model Apartment {
  id                      Int      @id @default(autoincrement())
  apartmentId             String
  address                 String
  factsheet               String?
  incidentalCosts         Int?
  state                   String
  moveInDate              DateTime?
  floor                   String
  floorNum                Int
  updatedAt               DateTime @updatedAt
  priceUnit               String
  rentalPrice            Int
  rentalPriceSquaremeterNet Int?
  rentalPriceSquaremeter  Int?
  title                   String
  name                    String
  rooms                   Int?
  area                   Int?
  virtualTourLink         String?
  balcony                Int?
  loggiaArea             Int?
  gardenSittingPlaceArea Int?
  terraceArea            Int?
  basementArea            Int?
  parkingSpaceCost       Int?
  deposit                Int?
  reduitArea            Int?
  referenceNumber        String?
}